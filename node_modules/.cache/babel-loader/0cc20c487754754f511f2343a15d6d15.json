v class=\"main\">\n    <div class=\"main_card-list\" v-for=\"dish in dishes\" :key=\"dish.id\">\n        <Card :dish=\"dish\" @add-cart=\"addItemCart\"></Card>\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport Card from '@/components/Card.vue';\nimport dishes from '@/services/dishes.json';\nimport store from '../store.js';\n\n\nexport default {\n  name: 'Main',\n  name: 'itemList',\n  components: {\n    Card,\n  },\n  data(){\n      return{\n       dishes,\n       itemList: store.itemList,\n      }\n  },\n  methods:{\n      addItemCart(dish){\n          const index = this.itemList.map(item => item.id).indexOf(dish.id);\n        if (index === -1) {\n            this.itemList.push({\n                data:dish,\n                quantity: 1,\n                id: dish.id,\n            })\n        } else {\n            this.itemList[index].qty += 1;\n        }\n        \n      }\n  }\n};\n</script>"],"sourceRoot":"src/components"}]}